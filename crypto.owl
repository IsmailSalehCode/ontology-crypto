    <!-- http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#AES -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#AES">
        <rdf:type rdf:resource="http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#DecryptionAlgorithm"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#EncryptionAlgorithm"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#KeyGenerationAlgorithm"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#SymmetricCryptographicMethod"/>
