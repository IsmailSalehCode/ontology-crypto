    <!-- http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#securityStrength -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ismail/ontologies/2024/0/crypto#securityStrength">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>medium</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
